<%- include('partials/header') %>

<nav class="navbar navbar-default" style="text-align: right;">
      <ul class="nav navbar-right mr-5">
        <li id="Username"><h1><%= username%></h1></li>
      </ul>
</nav>


<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">You've Discovered My Secret!</h1>
    <% userSecrets.forEach(function(user) { %>
      <p class="secret-text"><%= user.secret%></p>
    <% }); %>
    <hr>
    
    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button">Submit a Secret</a>
  </div>
</div>

<%- include('partials/footer') %>
